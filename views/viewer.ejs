<% include partials/header %>
    <div class="header">
        <h1>Find Your Park</h1>
    </div>

    <div class="content">
        <div class="pure-g">
            <% data['data'].forEach(function(park){ %>
                <h2 class="content-subheader pure-u-5-6">
                    <a href="<%= park['url'] %>"><%= park['fullName'] %></a>
                </h2>
                <h6 class="content-subheader pure-u-1-6 right-align"><%= park['designation'] %></h6>
                <p class="pure-u-1"><%= park['description'] %></p>
                <% if(park['images']){ %>
                    <div class="pure-u-1-4"></div>
                    <% let image = park['images'][0] %>
                        <div class="pure-u-1-2">
                            <p class="pure-u-1"><%= image['title'] %></p>
                            <img src="<%= image['url'] %>" alt="<%= image['altText'] %>">
                        </div>
                    <% } %>
                    <div class="pure-u-1-4"></div>
            <% }); %>
        </div>
    </div>

<% include partials/footer %>